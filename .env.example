# Environment example
# Copy to .env (NOT committed) and adjust values.

# Enable or disable the preview gate overlay (true|false)
VITE_ENABLE_PREVIEW_GATE=true

# Optional: override the preview access phrase (default: haslam)
VITE_PREVIEW_PASSWORD=haslam

# Application Insights (preferred: connection string)
# Replace with your real connection string OR instrumentation key.
# If both are set connection string wins.
VITE_APPINSIGHTS_CONNECTION_STRING=InstrumentationKey=3136dab6-1928-4e00-a020-0931ca72568a;IngestionEndpoint=https://dc.services.visualstudio.com/
# Legacy instrumentation key fallback (same GUID shown for illustration; normally only set one)
VITE_APPINSIGHTS_KEY=3136dab6-1928-4e00-a020-0931ca72568a
# Sampling percentage (0-100)
VITE_APPINSIGHTS_SAMPLE=50
# Disable web vitals collection (privacy / perf isolation)
VITE_DISABLE_VITALS=false
